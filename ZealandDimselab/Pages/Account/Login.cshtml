@page
@model ZealandDimselab.Pages.Account.LoginModel
@{
}
<div class="container">
    <div>
        @Model.Message
    </div>
    <form method="post">
        <div class="form-group">
            <label asp-for="Email" class="col-form-label "></label>
            <div>
                <input asp-for="Email" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Password" class="col-form-label"></label>
            <div>
                <input asp-for="Password" />
            </div>
        </div>
        <div class="form-group">
            <button class="btn btn-outline-light">Log in</button>
        </div>
    </form>
</div>